{
  "name": "codecloud/symfony-shopify-bundle",
  "type": "library",
  "description": "Makes Shopify app development a breeze",
  "keywords": ["shopify", "symfony"],
  "homepage": "https://github.com/richtermeister/symfony-shopify-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "David Smith",
      "email": "dave@code-cloud.uk",
      "homepage": "http://code-cloud.uk",
      "role": "Software Engineer"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "symfony/framework-bundle": "^3.0 || ^4.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "CodeCloud\\Bundle\\ShopifyBundle\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "6.2",
    "friendsofphp/php-cs-fixer": "^2.5"
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit"
    ],
    "cs-fixer": [
      "vendor/bin/php-cs-fixer fix ./src"
    ]
  }
}
